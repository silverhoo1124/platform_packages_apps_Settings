<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Status bar battery style selection dialog -->
    <!-- 通知栏电池图标样式选单 -->
    <string-array name="entries_status_bar_battery" translatable="false">
        <item>@string/battery_bar</item>
        <item>@string/battery_bar_percent</item>
        <item>@string/battery_circle</item>
        <item>@string/battery_circle_percent</item>
    </string-array>

    <string-array name="values_status_bar_battery" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <!-- Volume overlay style -->
    <!-- 音量调节面板样式选单 -->
    <string-array name="volume_overlay_entries">
        <item>Single</item>
        <item>Expandable</item>
        <item>Expanded</item>
        <item>None</item>
    </string-array>

    <string-array name="volume_overlay_values" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <!-- Volume rocker cursor control -->
    <!-- 音量键光标调节选单 -->
    <string-array name="volume_key_cursor_control_entries">
        <item>@string/volume_key_cursor_control_off</item>
        <item>@string/volume_key_cursor_control_on</item>
        <item>@string/volume_key_cursor_control_on_reverse</item>
    </string-array>

    <string-array name="volume_key_cursor_control_values">
        <item>0</item>
        <item>1</item>
        <item>2</item>
    </string-array>

    <!-- Increasing ring interval selection -->
    <!-- 渐强铃声间隔选单 -->
    <string-array name="increasing_ring_interval_entries" translatable="false">
        <item>@string/increasing_ring_interval_eachring</item>
        <item>@string/increasing_ring_interval_half_second</item>
        <item>@string/increasing_ring_interval_1second</item>
        <item>@string/increasing_ring_interval_2seconds</item>
        <item>@string/increasing_ring_interval_3seconds</item>
        <item>@string/increasing_ring_interval_5seconds</item>
        <item>@string/increasing_ring_interval_10seconds</item>
    </string-array>

    <integer-array name="increasing_ring_interval_values" translatable="false">
        <item>0</item>
        <item>500</item>
        <item>1000</item>
        <item>2000</item>
        <item>3000</item>
        <item>5000</item>
        <item>10000</item>
    </integer-array>

</resources>
